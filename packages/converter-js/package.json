{
  "name": "@nepalibhasha/converter",
  "version": "0.1.0",
  "description": "Legacy Nepali font to Unicode converter for browser and Node.js",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nepalibhasha/nepali-fonts.git",
    "directory": "packages/converter-js"
  },
  "author": "nepalibhasha",
  "keywords": [
    "nepali",
    "unicode",
    "converter",
    "devanagari",
    "preeti",
    "kantipur",
    "legacy-fonts"
  ],
  "devDependencies": {
    "typescript": "^5.7",
    "vitest": "^3.0",
    "tsup": "^8.0"
  },
  "license": "MIT"
}
