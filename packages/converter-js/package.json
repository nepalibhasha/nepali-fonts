{
  "name": "@nepalibhasha/converter",
  "version": "0.1.0",
  "description": "Legacy Nepali font to Unicode converter for browser and Node.js",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "typescript": "^5.7",
    "vitest": "^3.0",
    "tsup": "^8.0"
  },
  "license": "MIT"
}
